<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="256dp"
        android:onClick="seeAddFoodIntake"
        android:text="Add a meal or snack for the day"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="275dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="256dp"
        android:onClick="seeFoodBrowse"
        android:text="Browse Food Library"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="423dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="256dp"
        android:onClick="seePreGoals"
        android:text="Previous Nutrition Record"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="499dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="235dp"
        android:layout_height="70dp"
        android:gravity="center"
        android:text="Nutrition Dashboard"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="114dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="113dp"
        android:layout_height="70dp"
        app:srcCompat="@drawable/food_logo_2"
        tools:layout_editor_absoluteX="149dp"
        tools:layout_editor_absoluteY="32dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="341dp" />

</androidx.constraintlayout.widget.ConstraintLayout>